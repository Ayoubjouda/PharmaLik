{
  "expo": {
    "name": "pharmaLik",
    "slug": "pharmaLik",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "pharmaLik",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#0B7B56",
      "resizeMode": "contain"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PharmaLik to use your location."
        }
      ],
      "expo-localization",
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.PharmaLik",
      "config": {
        "googleMapsApiKey": "AIzaSyB5h6XQA1Idx4fZ6ZsSbJaK3vPIdqGhRU4"
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "088ae612-1aa5-4c50-8395-7c2435d55f77"
      }
    },
    "android": {
      "package": "com.example.PharmaLik",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB5h6XQA1Idx4fZ6ZsSbJaK3vPIdqGhRU4"
        }
      }
    }
  }
}

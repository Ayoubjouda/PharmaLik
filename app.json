{
  "expo": {
    "scheme": "pharmaLik",
    "version":"0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#0B7B56",
      "resizeMode": "contain"
    },
    "web": {
      "bundler": "metro"
    },
    "name": "pharmaLik",
    "slug": "pharmaLik",
    "plugins": [
      "expo-router",[
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PharmaLik to use your location."
        }
      ]
    ]
  }
}
